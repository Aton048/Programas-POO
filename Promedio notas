
int main() {
    double n1, n2, n3, promedio;

    cout << "Ingresa la nota 1: ";
    cin >> n1;
    cout << "Ingresa la nota 2: ";
    cin >> n2;
    cout << "Ingresa la nota 3: ";
    cin >> n3;

    promedio = (n1 + n2 + n3) / 3;

    cout << "El promedio es: " << promedio << endl;

    return 0;
}
